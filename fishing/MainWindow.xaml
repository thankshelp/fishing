<Window x:Class="fishing.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fishing"
        mc:Ignorable="d"
        Title="MainWindow" Height="1018" Width="1000">
    <Grid>
        <Grid x:Name="menu" HorizontalAlignment="Left" Height="1000" Margin="-4,-4,-2.4,0" VerticalAlignment="Top" Width="1000" Visibility="Visible">
        <Grid.Background>
            <ImageBrush ImageSource="image/fish_fon.jpg"/>
        </Grid.Background>
        <TextBox x:Name="kol_weed" HorizontalAlignment="Left" Height="44" Margin="333,272,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309"/>
        <TextBox x:Name="OFish" HorizontalAlignment="Left" Height="44" Margin="333,369,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309"/>
        <TextBox x:Name="BFish" HorizontalAlignment="Left" Height="44" Margin="333,466,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309"/>
        <TextBox x:Name="spB" HorizontalAlignment="Left" Height="44" Margin="333,666,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309"/>
        <TextBox x:Name="spO" HorizontalAlignment="Left" Height="44" Margin="333,569,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="309"/>
        <Label Content="Количество водорослей" HorizontalAlignment="Left" Height="36" Margin="333,231,0,0" VerticalAlignment="Top" Width="309" Foreground="White"/>
        <Label Content="Количество рыб 1" HorizontalAlignment="Left" Height="34" Margin="333,335,0,0" VerticalAlignment="Top" Width="309" Foreground="White"/>
        <Label Content="количество рыб 2" HorizontalAlignment="Left" Height="34" Margin="333,432,0,0" VerticalAlignment="Top" Width="309" Foreground="White"/>
        <Label Content="Скорость рыб 1" HorizontalAlignment="Left" Height="34" Margin="333,535,0,0" VerticalAlignment="Top" Width="309" Foreground="White"/>
        <Label Content="Скорость рыб 2" HorizontalAlignment="Left" Height="34" Margin="333,627,0,0" VerticalAlignment="Top" Width="309" Foreground="White"/>
        <Button x:Name="start" Content="Start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="121" Click="start_Click" Canvas.Left="58" Canvas.Top="51" Height="32" Margin="430,735,0,0"/>
        </Grid>
        <Canvas x:Name="scene" HorizontalAlignment="Left" Height="1000" Margin="-4,-4,-2.4,0" VerticalAlignment="Top" Width="1000" Visibility="Hidden">
            <Canvas.Background>
                <ImageBrush ImageSource="image/fish_fon.jpg"/>
            </Canvas.Background>
        </Canvas>
    </Grid>

   

</Window>
